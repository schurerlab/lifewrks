package edu.miami.ccs.life.saf;

/**
 * @author Sam Abeyruwan
 * @version 0.1
 *          <p/>
 *          Object model to handle the SAF to JSON schema
 */
public class SafJsonScalarObjectModel {

    private boolean required;
    private String  safId;
    private String  safType;
    private String  safDescription;
    private String  owlType;
    private String  baoLabel;
    private String  examples;

    public boolean isRequired() {
        return required;
    }

    public void setRequired(boolean required) {
        this.required = required;
    }

    public String getSafType() {
        return safType;
    }

    public void setSafType(String safType) {
        this.safType = safType;
    }

    public String getOwlType() {
        return owlType;
    }

    public void setOwlType(String owlType) {
        this.owlType = owlType;
    }

    public String getExamples() {
        return examples;
    }

    public void setExamples(String examples) {
        this.examples = examples;
    }

    public String getBaoLabel() {
        return baoLabel;
    }

    public void setBaoLabel(String baoLabel) {
        this.baoLabel = baoLabel;
    }

    public String getSafDescription() {
        return safDescription;
    }

    public void setSafDescription(String safDescription) {
        this.safDescription = safDescription;
    }

    public String getSafId() {
        return safId;
    }

    public void setSafId(String safId) {
        this.safId = safId;
    }
}
