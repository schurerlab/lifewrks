package edu.miami.ccs.life.config;

import java.util.List;

/**
 * @author Sam Abeyruwan
 * @version 0.1
 *          <p/>
 *          Handles LIFE framework configuration parameters
 */
public class ConfigurationDescription {

    private String                 safFilesBase;
    private String                 resultFilesBase;
    private String                 lifeOntology;
    private String                 tripleStore;
    private String                 safSchema;
    private String                 safExtension;
    private String                 resultExtension;
    private List<AssayTypeBinding> bioAssayTypeBindings;
    private List<AltEntry>         altEntries;
    private String                 mainNamespace;
    private String                 xlsxFile;
    private String                 safJsonFile;
    private String                 lifeMode;

    public ConfigurationDescription() {
    }

    public String getSafExtension() {
        return safExtension;
    }

    public void setSafExtension(String safExtension) {
        this.safExtension = safExtension;
    }

    public String getResultExtension() {
        return resultExtension;
    }

    public void setResultExtension(String resultExtension) {
        this.resultExtension = resultExtension;
    }

    public String getSafFilesBase() {
        return safFilesBase;
    }

    public void setSafFilesBase(String safFilesBase) {
        this.safFilesBase = safFilesBase;
    }

    public String getResultFilesBase() {
        return resultFilesBase;
    }

    public void setResultFilesBase(String resultFilesBase) {
        this.resultFilesBase = resultFilesBase;
    }

    public String getLifeOntology() {
        return lifeOntology;
    }

    public void setLifeOntology(String lifeOntology) {
        this.lifeOntology = lifeOntology;
    }

    public String getTripleStore() {
        return tripleStore;
    }

    public void setTripleStore(String tripleStore) {
        this.tripleStore = tripleStore;
    }

    public String getSafSchema() {
        return safSchema;
    }

    public void setSafSchema(String safSchema) {
        this.safSchema = safSchema;
    }

    public List<AssayTypeBinding> getBioAssayTypeBindings() {
        return bioAssayTypeBindings;
    }

    public void setBioAssayTypeBindings(List<AssayTypeBinding> bioAssayTypeBindings) {
        this.bioAssayTypeBindings = bioAssayTypeBindings;
    }

    public List<AltEntry> getAltEntries() {
        return altEntries;
    }

    public void setAltEntries(List<AltEntry> altEntries) {
        this.altEntries = altEntries;
    }

    public String getMainNamespace() {
        return mainNamespace;
    }

    public void setMainNamespace(String mainNamespace) {
        this.mainNamespace = mainNamespace;
    }

    public String getXlsxFile() {
        return xlsxFile;
    }

    public void setXlsxFile(String xlsxFile) {
        this.xlsxFile = xlsxFile;
    }

    public String getSafJsonFile() {
        return safJsonFile;
    }

    public void setSafJsonFile(String safJsonFile) {
        this.safJsonFile = safJsonFile;
    }

    public String getLifeMode() {
        return lifeMode;
    }

    public void setLifeMode(String lifeMode) {
        this.lifeMode = lifeMode;
    }
}
