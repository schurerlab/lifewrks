.tab_section label {
  margin-left: 0!important;
  margin-right: 0!important;
  font-size: 13px;
}
/**
 * tab navigator module
 */
.tabs_group .tabs_nav a.tab_item {
  color: #ffffff !important;
  text-align: center;
 /* width: 25%; */
  margin: 0;
  float: left;
  display: block;
  padding: .6em 0;
}
.tabs_group .tabs_nav a.tab_item.active {
  color: #ff8000 !important;
  background-color: #ffffff;
  border-top-right-radius: 3px;
  border-top-left-radius: 3px;
  -moz-border-radius-topright: 3px;
  -moz-border-radius-topleft: 3px;
  -webkit-border-top-right-radius: 3px;
  -webkit-border-top-left-radius: 3px;
}
.tab_item {
  width: 14%;
}
.tabs_group .tab_section {
  padding: 1em;
  display: none;
  opacity: 0;
}
.tabs_group .tab_section.active {
  opacity: 1;
  display: block;
}

/**
 * colored bar - used by tabs in view list
 */
.colored_bar {
  display: block;
  background-color: #49AFCD; /* #39a4e1; */
  border-radius: 3px;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  color: #ffffff;
  margin-bottom: 1em;
  overflow: hidden;
  padding: 3px 3px 0 3px;
  -webkit-box-shadow: inset 0 -4px 4px -2px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: inset 0 -4px 4px -2px rgba(0, 0, 0, 0.3);
  box-shadow: inset 0 -4px 4px -2px rgba(0, 0, 0, 0.3);
}

#resNotFoundMsg ul li {
    float: none;
    list-style-type: square;
    margin-left: 40px;
    line-height:1.5em;
}


